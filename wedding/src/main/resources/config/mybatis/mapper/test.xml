<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test">

	<select id="test" resultType="testBean">
		SELECT
			USER_SEQ AS userSeq
			, USER_ID AS userId
			, USER_PWD AS userPwd
		FROM CO_USER
	</select>


</mapper>












